
describe('Click on Selenium Java Course', function () {
  it('Visits Sauce School Landing Checks for Course Cards', function (){
    // cy.visit(`${Cypress.env('HOST_URL')}`)
    cy.visit("localhost:8080")
    cy.wait(500)
    // #cards > div > div:nth-child(1) > div > p:nth-child(3) > a
    cy.get('.link__button').first().click()
    cy.wait(500)
    cy.get('.description')
      .should('include', 'Module 1')
    })
})
